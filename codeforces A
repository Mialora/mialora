#include <iostream>
#include <cstdlib>
using namespace std;
 
int main()
{
	int a;
	int b;
	int i;
	cin >> a;
	for (i = 0; i < a; ++i){
		cin >> b;
		if (b == 2){
			cout << 1 << " " << 1;
		}
		else if (b % 2 == 1)
			cout << b - 1 << " " << 1;
		else
			cout << b - 2 << " " << 2;
		cout << endl;
	}
	return 0;
}
